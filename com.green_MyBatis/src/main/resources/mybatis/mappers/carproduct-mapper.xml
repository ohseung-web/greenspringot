<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.green.carproduct.mapper.CarProductMapper">

<!-- 상품의 모든 정보를 검색하는 쿼리문 : selelct문 -->
  <select id="getAllCarProduct" resultType="carDTO" >
  	SELECT * FROM carProduct
  </select>
  
  <insert id="insertCarProduct" parameterType="carDTO">
    INSERT INTO carproduct
    (carName, price, company, img, info)
    VALUES
    (#{carName}, #{price}, #{company}, #{img}, #{info})
</insert>


</mapper>